{
  "expo": {
    "name": "Flipship",
    "slug": "Flipship",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "splash": {
      "image": "./assets/images/transparent-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#005CB8"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#005CB8"
      },
      "package": "com.testertesting9112001.Flipship"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/transparent-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#005CB8"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "77f3f710-05ce-4961-95a2-382971f1b581"
      }
    }
  }
}
